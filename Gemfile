source "https://rubygems.org"

gem "jekyll"

# Windows and JRuby does not include zoneinfo files, so bundle the tzinfo-data gem and associated library.
platforms :mingw, :x64_mingw, :windows, :jruby do
  gem "tzinfo"
  gem "tzinfo-data"
end

gem "webrick"
gem 'html-proofer'
gem 'jekyll-gist'
gem 'rouge'
